<%- model_class = RobotLog -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
<dt><strong><%= model_class.human_attribute_name(:message) %>:</strong></dt>
  <dd><%= @robot_log.message %></dd>
  <dt><strong>Chats Feitos:</strong></dt>
  <dd><%= @robot_log.done_chat %></dd>
  <dt><strong>Chats Erros:</strong></dt>
  <dd><%= @robot_log.fail_chat %></dd>
  <dt><strong><%= model_class.human_attribute_name(:robot_id) %>:</strong></dt>
  <dd><%= @robot_log.robot.name %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              robot_logs_path, :class => 'btn btn-default'  %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              robot_log_path(@robot_log),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
